{
  "name": "gitcompass",
  "version": "1.0.0",
  "description": "AI-Powered Open Source GitHub Repository Finder",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:ai": "cd ai-engine && python main.py",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "build": "cd frontend && npm run build",
    "start": "npm run dev:backend",
    "start:ai": "cd ai-engine && uvicorn main:app --host 0.0.0.0 --port 8000"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "github",
    "open-source",
    "ai",
    "nlp",
    "repository-finder",
    "resume-parser"
  ],
  "author": "",
  "license": "MIT"
}
